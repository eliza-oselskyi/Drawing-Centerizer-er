<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>latestmajor</LangVersion>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\FriendAssemblies.snk</AssemblyOriginatorKeyFile>
    <PublicSign>true</PublicSign>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Tekla.Structures" Version="2023.0.0" />
    <PackageReference Include="Tekla.Structures.Drawing" Version="2023.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\FriendAssemblies.snk">
      <Link>FriendAssemblies.snk</Link>
    </None>
  </ItemGroup>

</Project>
